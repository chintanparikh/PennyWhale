= render 'navbar'
.container.fill#execute-container
  = render 'search_form'
  = render 'app/queries/components/errors' unless flash[:query_error_array].nil? and flash[:query_warning_array].nil?
  - if flash[:query_warning_array].nil? and flash[:query_error_array].nil?
    .col-lg-2
    .col-lg-8.fill.panel.panel-default#query-answer-panel
      .panel-body
        = render 'app/queries/components/main'
        %hr
        = render 'app/queries/components/graph'
        %hr
        = render 'app/queries/components/news'
    .col-lg-2
